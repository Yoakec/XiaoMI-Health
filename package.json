  "dependencies": {
    "@unocss/preset-attributify": "^0.46.5",
    "@unocss/preset-icons": "^0.46.5",
    "@unocss/preset-uno": "^0.46.5",
    "@vueuse/core": "^9.5.0",
    "echarts": "^5.4.0",
    "element-plus": "^2.2.21",
    "sass": "^1.56.1",
    "unocss": "^0.46.5",
